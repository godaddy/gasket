{
  "name": "@gasket/plugin-workbox",
  "version": "7.3.3",
  "description": "Gasket Workbox Plugin",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": "godaddy/gasket.git",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "plugin",
    "workbox"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": "https://github.com/godaddy/gasket/issues",
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-workbox",
  "dependencies": {
    "deepmerge": "^4.3.1",
    "serve-static": "^1.16.2",
    "workbox-build": "^4.3.1"
  },
  "devDependencies": {
    "@gasket/core": "workspace:*",
    "@gasket/plugin-command": "workspace:*",
    "@gasket/plugin-express": "workspace:*",
    "@gasket/plugin-fastify": "workspace:*",
    "@gasket/plugin-logger": "workspace:*",
    "@gasket/plugin-metadata": "workspace:*",
    "@gasket/plugin-service-worker": "workspace:*",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.19",
    "@types/workbox-build": "^5.1.0",
    "create-gasket-app": "workspace:*",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-config-godaddy-typescript": "^4.0.3",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "typescript": "^5.7.3"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "lib/*.ts"
        ],
        "extends": [
          "godaddy-typescript"
        ],
        "rules": {
          "jsdoc/*": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "__mocks__"
  ]
}
