{
  "name": "@gasket/plugin-docusaurus",
  "version": "6.28.0",
  "description": "Gasket plugin for docusaurus",
  "main": "lib",
  "files": [
    "lib",
    "generator"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha test/*.test.js --require test/setup.js",
    "test:watch": "npm run test:runner -- --watch",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "report": "nyc report --reporter=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "docs",
    "viewer",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Michael Mason <mmason2@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/",
  "dependencies": {
    "@docusaurus/core": "2.1.0",
    "@docusaurus/preset-classic": "2.1.0",
    "lodash.defaultsdeep": "^4.6.1"
  },
  "devDependencies": {
    "@gasket/engine": "^6.26.1",
    "assume": "^2.3.0",
    "assume-sinon": "^1.1.0",
    "eslint": "^8.7.0",
    "eslint-config-godaddy": "^6.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-unicorn": "^40.1.0",
    "mocha": "^9.1.4",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^12.0.1"
  },
  "eslintConfig": {
    "extends": [
      "godaddy"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  }
}
