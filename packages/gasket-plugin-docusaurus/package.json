{
  "name": "@gasket/plugin-docusaurus",
  "version": "7.0.0-next.48",
  "description": "Gasket plugin for docusaurus",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "generator"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint && npm run typecheck",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "docs",
    "viewer",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Michael Mason <mmason2@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/",
  "dependencies": {
    "lodash.defaultsdeep": "^4.6.1"
  },
  "devDependencies": {
    "@docusaurus/core": "^3.4.0",
    "@docusaurus/preset-classic": "^3.4.0",
    "@gasket/core": "^7.0.0-next.48",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "search-insights": "^2.13.0",
    "typescript": "^5.4.5"
  },
  "disabled_peerDependencies": {
    "@gasket/plugin-logger": "^7"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  }
}
