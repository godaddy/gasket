{
  "name": "@gasket/plugin-docusaurus",
  "version": "7.5.0",
  "description": "Gasket plugin for docusaurus",
  "type": "module",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "cjs",
    "generator",
    "EXAMPLES.md"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./cjs/index.cjs",
      "default": "./cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "gasket-cjs ./lib",
    "build:watch": "pnpm run build --watch",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run --globals",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": "godaddy/gasket.git",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "docs",
    "gasket",
    "plugin",
    "viewer"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": "https://github.com/godaddy/gasket/issues",
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-docusaurus",
  "devDependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@gasket/cjs": "workspace:^",
    "@gasket/core": "workspace:^",
    "@gasket/plugin-docs": "workspace:^",
    "@gasket/plugin-logger": "workspace:^",
    "@gasket/plugin-metadata": "workspace:^",
    "@gasket/plugin-webpack": "workspace:^",
    "@vitest/coverage-v8": "^3.2.0",
    "ajv": "^8.17.1",
    "create-gasket-app": "workspace:^",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "search-insights": "^2.17.3",
    "typescript": "^5.8.2",
    "vitest": "^3.2.0"
  },
  "peerDependencies": {
    "@gasket/plugin-logger": "^7.4.0"
  }
}
