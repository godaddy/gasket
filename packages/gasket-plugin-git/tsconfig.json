{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "types": [
      "@types/jest",
      "@types/node",
    ]
  },
  "exclude": [
    "test",
    "coverage",
    "generator"
  ]
}
