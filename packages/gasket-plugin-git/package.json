{
  "name": "@gasket/plugin-git",
  "version": "6.15.0",
  "description": "Adds git support to your application",
  "main": "lib",
  "files": [
    "generator",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha --require ./test/setup.js \"test/**/*.*(test|spec).js\"",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "report": "nyc report --reporter=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "git",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Sivan Mehta <smehta@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-git",
  "dependencies": {
    "@gasket/utils": "^6.15.0"
  },
  "devDependencies": {
    "assume": "^2.3.0",
    "assume-sinon": "^1.1.0",
    "eslint": "^8.7.0",
    "eslint-config-godaddy": "^6.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-unicorn": "^40.1.0",
    "mocha": "^9.1.4",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^12.0.1"
  },
  "eslintConfig": {
    "extends": [
      "godaddy"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  }
}
