{
  "name": "@gasket/plugin-docs-graphs",
  "version": "6.0.12",
  "description": "Generate mermaid graphs of an applications gasket lifecycles",
  "main": "plugin.js",
  "scripts": {
    "lint": "eslint *.js test",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha test/*.test.js",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "docs",
    "gasket",
    "plugin",
    "mermaid"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Sivan Mehta <smehta@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-plugin-doc-graph",
  "devDependencies": {
    "assume": "^2.3.0",
    "eslint": "^7.17.0",
    "eslint-config-godaddy": "^4.0.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  }
}
