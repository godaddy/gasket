{
  "compilerOptions": {
    "target": "es2015",
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "types": [
      "@types/jest"
    ]
  },
  "exclude": [
    "test",
    "coverage",
    "generator"
  ]
}
