{
  "name": "@gasket/plugin-nextjs",
  "version": "7.0.0-next.3",
  "description": "Adds Next support to your application",
  "main": "lib",
  "types": "lib/index.d.ts",
  "files": [
    "generator",
    "docs",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "next",
    "nextjs",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-nextjs",
  "dependencies": {
    "@gasket/plugin-webpack": "^7.0.0-next.3",
    "@gasket/resolve": "^7.0.0-next.3",
    "fastify": "^4.26.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.23.3",
    "@gasket/assets": "^7.0.0-next.3",
    "@gasket/engine": "^7.0.0-next.3",
    "@gasket/nextjs": "^7.0.0-next.3",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.0.2",
    "eslint-config-godaddy-react": "^9.0.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^44.0.0",
    "jest": "^29.7.0",
    "lodash.merge": "^4.6.2",
    "next": "^14.0.0",
    "next-redux-wrapper": "^8.0.0",
    "nodemon": "^3.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "disabled_peerDependencies": {
    "@gasket/plugin-webpack": "^7"
  },
  "peerDependencies": {
    "next": ">=10.2.0 <= 13.1.1 || ^14",
    "prop-types": "^15",
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended"
    ],
    "globals": {
      "expect": "readonly"
    },
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "generator/**/*.js"
        ],
        "extends": [
          "godaddy-react"
        ]
      }
    ]
  },
  "eslintIgnore": [
    "generator/redux/redux/store.js",
    "generator/app/pages/_app.js",
    "*.d.ts"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "jest": {
    "rootDir": "test"
  }
}
