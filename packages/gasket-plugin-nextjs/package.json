{
  "name": "@gasket/plugin-nextjs",
  "version": "7.7.2",
  "description": "Adds Next support to your application",
  "type": "module",
  "types": "lib/index.d.ts",
  "files": [
    "generator",
    "docs",
    "lib",
    "cjs",
    "EXAMPLES.md"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./cjs/index.cjs",
      "default": "./cjs/index.cjs"
    },
    "./prompts": {
      "import": "./lib/prompt.js",
      "require": "./cjs/prompt.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "gasket-cjs ./lib && node scripts/fix-cjs-paths.js",
    "build:watch": "pnpm run build --watch",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run --globals",
    "test:coverage": "vitest run --globals --coverage",
    "test:watch": "vitest --globals",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": "godaddy/gasket.git",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "next",
    "nextjs",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": "https://github.com/godaddy/gasket/issues",
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-nextjs",
  "dependencies": {
    "@gasket/plugin-webpack": "workspace:^",
    "fastify": "^4.29.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.26.3",
    "@gasket/assets": "workspace:^",
    "@gasket/cjs": "workspace:^",
    "@gasket/core": "workspace:^",
    "@gasket/intl": "workspace:^",
    "@gasket/nextjs": "workspace:^",
    "@gasket/plugin-elastic-apm": "workspace:^",
    "@gasket/plugin-express": "workspace:^",
    "@gasket/plugin-fastify": "workspace:^",
    "@gasket/plugin-https": "workspace:^",
    "@gasket/plugin-intl": "workspace:^",
    "@gasket/plugin-logger": "workspace:^",
    "@gasket/plugin-metadata": "workspace:^",
    "@gasket/plugin-nextjs": "workspace:^",
    "@gasket/plugin-service-worker": "workspace:^",
    "@gasket/plugin-workbox": "workspace:^",
    "@gasket/request": "workspace:^",
    "@types/express": "^4.17.23",
    "@types/node": "^20.17.19",
    "@types/react": "^19.0.12",
    "@types/webpack": "^5.28.5",
    "create-gasket-app": "workspace:^",
    "lodash.merge": "^4.6.2",
    "next": "^15.5.10",
    "nodemon": "^3.1.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vitest": "^3.2.0"
  },
  "peerDependencies": {
    "@gasket/plugin-webpack": "^7.4.0",
    "react": ">=18 <20",
    "react-dom": ">=18 <20"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  }
}
