{
  "name": "@gasket/plugin-nextjs",
  "version": "7.1.0-next.5",
  "description": "Adds Next support to your application",
  "types": "lib/index.d.ts",
  "files": [
    "generator",
    "docs",
    "lib"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./prompts": "./lib/prompt.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "posttest": "npm run lint && npm run typecheck",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "next",
    "nextjs",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-nextjs",
  "dependencies": {
    "@gasket/plugin-webpack": "7.1.0-next.5",
    "fastify": "^4.28.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.23.3",
    "@gasket/assets": "7.1.0-next.5",
    "@gasket/core": "7.1.0-next.5",
    "@gasket/nextjs": "7.1.0-next.5",
    "@types/react": "^18.2.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-config-godaddy-react": "^9.1.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "lodash.merge": "^4.6.2",
    "next": "^14.0.0",
    "nodemon": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.4.5"
  },
  "disabled_peerDependencies": {
    "@gasket/plugin-webpack": "^7",
    "next": "^14",
    "react": "^18",
    "react-dom": "^18"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "globals": {
      "expect": "readonly"
    },
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "generator/**/*.js"
        ],
        "extends": [
          "godaddy-react"
        ]
      }
    ]
  },
  "eslintIgnore": [
    "generator",
    "*.d.ts"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "jest": {
    "rootDir": "test"
  }
}
