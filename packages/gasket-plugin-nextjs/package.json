{
  "name": "@gasket/plugin-nextjs",
  "version": "6.36.0",
  "description": "Adds Next support to your application",
  "main": "lib",
  "types": "lib/index.d.ts",
  "files": [
    "generator",
    "docs",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha --require ./test/setup.js \"test/**/*.test.js\"",
    "test:watch": "npm run test:runner -- --watch",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "report": "nyc report --reporter=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "next",
    "nextjs",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-nextjs",
  "dependencies": {
    "@gasket/plugin-webpack": "^6.36.0",
    "@gasket/resolve": "^6.36.0",
    "fastify": "^4.1.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.10",
    "@gasket/assets": "^6.35.0",
    "@gasket/engine": "^6.36.0",
    "@gasket/nextjs": "^6.35.0",
    "assume": "^2.3.0",
    "assume-sinon": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.7.0",
    "eslint-config-godaddy": "^6.0.0",
    "eslint-config-godaddy-react": "^8.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-unicorn": "^44.0.0",
    "lodash.merge": "^4.6.0",
    "mocha": "^10.0.0",
    "next": "^12.0.8",
    "next-redux-wrapper": "^8.0.0",
    "nyc": "^15.1.0",
    "prop-types": "^15.7.2",
    "proxyquire": "^2.1.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sinon": "^14.0.0"
  },
  "peerDependencies": {
    "next": ">=10.2.0 < 13",
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  },
  "eslintIgnore": [
    "generator/redux/redux/store.js",
    "generator/app/pages/_app.js",
    "*.d.ts"
  ],
  "eslintConfig": {
    "extends": [
      "godaddy"
    ],
    "globals": {
      "expect": "readonly"
    },
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "generator/**/*.js"
        ],
        "extends": [
          "godaddy-react"
        ]
      }
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  }
}
