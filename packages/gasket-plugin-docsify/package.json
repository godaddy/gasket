{
  "name": "@gasket/plugin-docsify",
  "version": "5.0.0",
  "description": "View collated docs with Docsify",
  "main": "lib",
  "scripts": {
    "lint": "eslint lib test",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha test/*.test.js --require test/setup.js",
    "test:watch": "npm run test:runner -- --watch",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "report": "nyc report --reporter=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "docs",
    "viewer",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-plugin-docsify",
  "dependencies": {
    "docsify": "^4.10.2",
    "docsify-cli": "^4.3.0",
    "glob": "^7.1.4",
    "handlebars": "^4.4.3",
    "lodash.defaultsdeep": "^4.6.0",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "assume": "^2.2.0",
    "eslint": "^6.1.0",
    "eslint-config-godaddy": "^4.0.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^6.0.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "sinon": "^7.4.1"
  }
}
