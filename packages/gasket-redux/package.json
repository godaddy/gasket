{
  "name": "@gasket/redux",
  "version": "7.3.0",
  "description": "Gasket Redux Configuration",
  "main": "lib",
  "types": "src/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch",
    "build": "babel src -d lib --ignore \"**/*.spec.js\" --delete-dir-on-start",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "redux"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-redux",
  "dependencies": {
    "@babel/runtime": "catalog:",
    "redux-logger": "catalog:",
    "redux-thunk": "catalog:"
  },
  "devDependencies": {
    "@babel/cli": "catalog:",
    "@babel/core": "catalog:",
    "@babel/plugin-proposal-object-rest-spread": "catalog:",
    "@babel/plugin-transform-runtime": "catalog:",
    "@babel/preset-env": "catalog:",
    "@gasket/plugin-logger": "workspace:^",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "babel-core": "catalog:",
    "babel-eslint": "catalog:",
    "babel-jest": "catalog:",
    "cross-env": "catalog:",
    "eslint": "catalog:",
    "eslint-config-godaddy": "catalog:",
    "eslint-config-godaddy-typescript": "catalog:",
    "eslint-plugin-jest": "catalog:",
    "eslint-plugin-jsx-a11y": "catalog:",
    "eslint-plugin-unicorn": "catalog:",
    "jest": "catalog:",
    "jest-environment-jsdom": "catalog:",
    "redux": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "redux": "^3.7.2 || ^4.0.1"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "lib/*.ts"
        ],
        "extends": [
          "godaddy-typescript"
        ],
        "rules": {
          "jsdoc/*": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "lib"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/transform-runtime",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  }
}
