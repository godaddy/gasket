{
  "name": "@gasket/plugin-morgan",
  "version": "7.0.0-next.62",
  "description": "Adds morgan request logger to your app",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint && npm run typecheck",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "morgan",
    "express",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Paulo D Goncalves <v_pgoncalves@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-morgan",
  "dependencies": {
    "morgan": "^1.10.0",
    "split": "^1.0.1"
  },
  "devDependencies": {
    "@gasket/core": "7.0.0-next.62",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "typescript": "^5.4.5"
  },
  "disabled_peerDependencies": {
    "@gasket/plugin-logger": "^7"
  },
  "peerDependencies": {
    "express": "^4.16.3"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "ignorePatterns": "*.d.ts"
  }
}
