{
  "name": "@gasket/nextjs-plugin",
  "version": "2.0.1",
  "description": "Adds Next support to your application",
  "author": "GoDaddy Operating Company, LLC",
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-nextjs-plugin",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "lint": "eslint './**/*.js' 'test/**/*.js'",
    "lint:fix": "npm run lint -- --fix",
    "report": "nyc report --reporter=lcov",
    "test": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "test:runner": "mocha --require ./setup.js 'test/**/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket"
  },
  "keywords": [
    "next",
    "nextjs",
    "gasket",
    "plugin"
  ],
  "dependencies": {
    "@gasket/webpack-plugin": "^1.0.2"
  },
  "peerDependencies": {
    "next": "^7.0.0 || ^8.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "devDependencies": {
    "@gasket/plugin-engine": "^1.3.1",
    "assume": "^2.1.0",
    "assume-sinon": "^1.0.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.13.0",
    "eslint-config-godaddy": "^3.0.0",
    "eslint-plugin-json": "^1.3.2",
    "eslint-plugin-mocha": "^5.2.1",
    "mocha": "^5.2.0",
    "next": "^8.1.0",
    "nyc": "^13.1.0",
    "proxyquire": "^2.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sinon": "^7.2.3"
  }
}
