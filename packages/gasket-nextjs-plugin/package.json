{
  "name": "@gasket/nextjs-plugin",
  "version": "2.3.1",
  "description": "Adds Next support to your application",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js \"{test,generator}/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha --require ./setup.js \"test/**/*.test.js\"",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "report": "nyc report --reporter=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "keywords": [
    "next",
    "nextjs",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-nextjs-plugin",
  "dependencies": {
    "@gasket/webpack-plugin": "^1.1.0"
  },
  "devDependencies": {
    "@gasket/plugin-engine": "^2.0.1",
    "assume": "^2.2.0",
    "assume-sinon": "^1.0.1",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-config-godaddy": "^4.0.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^6.0.0",
    "mocha": "^6.2.0",
    "next": "^9.0.4",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sinon": "^7.4.1"
  },
  "peerDependencies": {
    "next": ">=7.0.0 <10.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  }
}
