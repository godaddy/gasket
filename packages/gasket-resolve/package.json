{
  "name": "@gasket/resolve",
  "version": "6.46.2",
  "description": "Essential module resolution & configuration management for gasket plugins & presets.",
  "main": "lib",
  "types": "types",
  "files": [
    "lib",
    "docs",
    "types"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint",
    "docs": "jsdoc2md --plugin @godaddy/dmd --files lib/*.js > docs/api.md"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "resolve",
    "preset",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Charlie Robbins <charlie.robbins@gmail.com>",
    "Jacob Page <jpage@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-resolve",
  "dependencies": {
    "diagnostics": "^2.0.2",
    "lodash.defaultsdeep": "^4.6.1",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@gasket/engine": "^6.46.2",
    "@godaddy/dmd": "^1.0.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.0.2",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^44.0.0",
    "jest": "^29.7.0",
    "jsdoc-to-markdown": "^7.1.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  },
  "eslintIgnore": [
    "test/fixtures"
  ],
  "jest": {
    "testEnvironment": "node"
  }
}
