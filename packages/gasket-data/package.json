{
  "name": "@gasket/data",
  "version": "7.3.3",
  "description": "Helper package for accessing embedded Gasket Data in the browser",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "pnpm run clean && pnpm run build:esm && pnpm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "lint": "eslint src test",
    "lint:fix": "pnpm run lint --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "test": "vitest --watch=false",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "prepublishOnly": "pnpm run build"
  },
  "repository": "godaddy/gasket.git",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "data",
    "gasket",
    "helper"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": "https://github.com/godaddy/gasket/issues",
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-data",
  "dependencies": {
    "@gasket/request": "workspace:*"
  },
  "devDependencies": {
    "@gasket/plugin-data": "workspace:*",
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.18",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.19",
    "@vitest/coverage-v8": "3.0.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-config-godaddy-typescript": "^4.0.3",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "jsdom": "^20.0.3",
    "typescript": "^5.7.3"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "dist/"
    ],
    "extends": [
      "godaddy-typescript",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error",
      "jsdoc/require-param": "off",
      "jsdoc/require-returns": "off"
    }
  }
}
