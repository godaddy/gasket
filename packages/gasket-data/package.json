{
  "name": "@gasket/data",
  "version": "7.3.0",
  "description": "Helper package for accessing embedded Gasket Data in the browser",
  "type": "module",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "cjs"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./cjs/index.cjs",
      "types": "./lib/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict --experimental-vm-modules' jest",
    "test:watch": "pnpm run test --watch",
    "test:coverage": "pnpm run test --coverage",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch",
    "build": "swc lib -d cjs --delete-dir-on-start --strip-leading-paths --out-file-extension cjs",
    "postbuild": "node -e \"require('fs').writeFileSync('cjs/package.json', '{}')\"",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "helper",
    "data"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-data",
  "dependencies": {
    "@gasket/request": "workspace:^"
  },
  "devDependencies": {
    "@gasket/plugin-data": "workspace:^",
    "@jest/globals": "catalog:",
    "@swc/cli": "catalog:",
    "@swc/core": "catalog:",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "cross-env": "catalog:",
    "eslint": "catalog:",
    "eslint-config-godaddy": "catalog:",
    "eslint-config-godaddy-typescript": "catalog:",
    "eslint-plugin-jest": "catalog:",
    "eslint-plugin-unicorn": "catalog:",
    "jest": "catalog:",
    "jsdom": "catalog:",
    "typescript": "catalog:"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "cjs/"
    ],
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "lib/*.ts"
        ],
        "extends": [
          "godaddy-typescript"
        ],
        "rules": {
          "jsdoc/*": "off"
        }
      }
    ]
  },
  "jest": {
    "transform": {}
  }
}
