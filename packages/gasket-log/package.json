{
  "name": "@gasket/log",
  "version": "6.38.0",
  "description": "Gasket client and server logger",
  "main": "src/server.js",
  "browser": "lib/client.js",
  "module": "src/client.js",
  "types": "src/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' npm run test:runner",
    "test:runner": "npm run test:client && npm run test:server",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:client": "jest test/client.test.js",
    "test:server": "jest test/server.test.js",
    "posttest": "npm run lint",
    "build": "babel ./src/client.js -d lib --delete-dir-on-start",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "log",
    "client"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Martijn Swaagman <mswaagman@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-log",
  "dependencies": {
    "diagnostics": "^2.0.2",
    "kuler": "^2.0.0",
    "triple-beam": "^1.3.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.16.10",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.12.10",
    "@babel/register": "^7.12.10",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-godaddy": "^7.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "jest": "^29.3.1"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "unicorn",
      "babel"
    ],
    "rules": {
      "unicorn/filename-case": "error",
      "no-return-assign": 0,
      "no-cond-assign": 0,
      "strict": 0
    }
  },
  "eslintIgnore": [
    "lib"
  ],
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  }
}
