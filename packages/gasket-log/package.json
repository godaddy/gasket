{
  "name": "@gasket/log",
  "version": "3.0.0",
  "description": "Gasket client and server logger",
  "main": "./server.js",
  "browser": "./lib/client.js",
  "module": "./client.js",
  "scripts": {
    "lint": "eslint --fix *.js test/*.js",
    "test": "npm run test:client && npm run test:server",
    "posttest": "npm run lint",
    "build": "babel ./client.js -d lib",
    "prepublishOnly": "npm run build",
    "test:client": "DEBUG=gasket:* nyc mocha --require @babel/register test/client.test.js",
    "test:server": "nyc mocha test/server.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "keywords": [
    "gasket",
    "log",
    "client"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Martijn Swaagman <mswaagman@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-log#readme",
  "dependencies": {
    "diagnostics": "^2.0.2",
    "kuler": "^2.0.0",
    "triple-beam": "^1.3.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "assume": "^2.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-config-godaddy": "^3.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-json": "^1.3.2",
    "eslint-plugin-mocha": "^5.2.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "sinon": "^7.2.3"
  }
}
