{
  "name": "@gasket/plugin-webpack",
  "version": "6.20.1",
  "description": "Adds webpack support to your application",
  "main": "lib",
  "types": "lib/index.d.ts",
  "files": [
    "docs",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha --require ./test/setup.js \"test/**/*.test.js\"",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "test:watch": "npm run test:runner -- --watch",
    "posttest": "npm run lint",
    "report": "nyc report --reporter=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "webpack",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-webpack",
  "dependencies": {
    "webpack-chain": "^6.0.0",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@gasket/engine": "^6.19.0",
    "assume": "^2.3.0",
    "assume-sinon": "^1.1.0",
    "eslint": "^8.7.0",
    "eslint-config-godaddy": "^6.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-unicorn": "^40.1.0",
    "mocha": "^9.1.4",
    "mock-require": "^3.0.3",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^12.0.1",
    "webpack": "^5.21.2"
  },
  "eslintConfig": {
    "extends": [
      "godaddy"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  }
}
