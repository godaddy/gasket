{
  "name": "@gasket/assets",
  "version": "6.34.4",
  "description": "Gasket assets",
  "main": "react",
  "files": [
    "react",
    "svgs"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint",
    "build": "npm run clean && npm run svg-to-react",
    "prepublishOnly": "npm run build",
    "clean": "rimraf ./react",
    "svg-to-react": "babel-node --presets @babel/env ./scripts/svg-to-react.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "assets",
    "logo"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-assets",
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@svgr/core": "^6.2.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "eslint": "^8.7.0",
    "eslint-config-godaddy": "^6.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-unicorn": "^43.0.0",
    "mkdirp": "^1.0.4",
    "react": "^17.0.1",
    "recursive-readdir": "^2.2.2",
    "rimraf": "^3.0.2"
  },
  "eslintConfig": {
    "extends": [
      "godaddy"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  },
  "eslintIgnore": [
    "react"
  ]
}
