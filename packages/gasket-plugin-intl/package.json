{
  "name": "@gasket/plugin-intl",
  "version": "6.8.3",
  "description": "NodeJS script to build localization files.",
  "main": "lib",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha --require setup-env \"test/*.test.js\"",
    "test:watch": "npm run test:runner -- --watch",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "plugin",
    "intl"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Salil Agrawal <sagrawal@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-plugin-intl",
  "dependencies": {
    "@gasket/helper-intl": "^6.8.0",
    "@hapi/accept": "^5.0.1",
    "fs-extra": "^5.0.0",
    "intl": "^1.2.5",
    "loader-utils": "^1.1.0",
    "lodash.merge": "^4.6.0",
    "serve-static": "^1.14.1",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@gasket/engine": "^6.7.3",
    "@gasket/plugin-log": "^6.1.0",
    "@gasket/react-intl": "^6.8.0",
    "assume": "^2.3.0",
    "assume-sinon": "^1.1.0",
    "eslint": "^7.17.0",
    "eslint-config-godaddy": "^4.0.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "react-intl": "^5.10.13",
    "setup-env": "^1.2.2",
    "sinon": "^9.2.3"
  },
  "peerDependencies": {
    "@gasket/plugin-log": "^6.0.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  },
  "eslintIgnore": [
    "coverage"
  ]
}
