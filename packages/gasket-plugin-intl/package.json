{
  "name": "@gasket/plugin-intl",
  "version": "7.0.0-next.40",
  "description": "NodeJS script to build localization files.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "generator",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint && npm run typecheck",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "plugin",
    "intl"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Salil Agrawal <sagrawal@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-intl",
  "dependencies": {
    "@gasket/helper-intl": "^7.0.0-next.40",
    "@hapi/accept": "^6.0.3",
    "debug": "^4.3.4",
    "fs-extra": "^10.0.0",
    "glob": "^8.1.0",
    "intl": "^1.2.5",
    "loader-utils": "^3.2.1",
    "lodash.merge": "^4.6.2",
    "semver": "^7.5.4",
    "serve-static": "^1.15.0",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@gasket/core": "^7.0.0-next.40",
    "@gasket/react-intl": "^7.0.0-next.40",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.1.0",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^44.0.0",
    "jest": "^29.7.0",
    "react-intl": "^6.0.0",
    "typescript": "^5.4.5",
    "webpack": "^5.89.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  }
}
