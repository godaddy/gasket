{
  "name": "@gasket/preset-api",
  "version": "7.1.3",
  "description": "Create Express-based API with Gasket",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "generator",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict --experimental-vm-modules' jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-preset-api",
  "dependencies": {
    "@gasket/plugin-cypress": "^7.1.2",
    "@gasket/plugin-docs": "^7.1.2",
    "@gasket/plugin-dynamic-plugins": "^7.1.2",
    "@gasket/plugin-express": "^7.1.3",
    "@gasket/plugin-fastify": "^7.1.2",
    "@gasket/plugin-https": "^7.1.2",
    "@gasket/plugin-jest": "^7.1.0",
    "@gasket/plugin-lint": "^7.1.3",
    "@gasket/plugin-mocha": "^7.1.0",
    "@gasket/plugin-swagger": "^7.1.2",
    "@gasket/plugin-typescript": "^7.1.0",
    "@gasket/plugin-winston": "^7.1.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  },
  "eslintIgnore": [
    "test/fixtures",
    "generator"
  ],
  "jest": {
    "transform": {}
  }
}
