{
  "name": "create-gasket-app",
  "version": "6.0.0-canary.2",
  "description": "starter pack for creating a gasket app",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha test/*.test.js --require test/setup.js",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint"
  },
  "bin": {
    "create-gasket-app": "./index.js"
  },
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "dependencies": {
    "@gasket/cli": "^6.0.0-canary.2"
  },
  "devDependencies": {
    "assume": "^2.2.0",
    "assume-sinon": "^1.0.1",
    "chai": "^4.2.0",
    "cross-env": "^5.1.6",
    "eslint": "^6.1.0",
    "eslint-config-godaddy": "^4.0.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^6.0.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "sinon": "^7.4.1"
  },
  "eslintConfig": {
    "extends": [
      "godaddy"
    ]
  },
  "eslintIgnore": [
    "coverage",
    "test/fixtures"
  ]
}
