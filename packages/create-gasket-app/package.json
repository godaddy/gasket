{
  "name": "create-gasket-app",
  "version": "7.3.0",
  "description": "starter pack for creating a gasket app",
  "type": "module",
  "bin": {
    "create-gasket-app": "./lib/index.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict --experimental-vm-modules' jest",
    "test:watch": "pnpm run test --watch",
    "test:coverage": "jest --coverage",
    "posttest": "pnpm run lint",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/create-gasket-app",
  "dependencies": {
    "@gasket/core": "workspace:^",
    "@gasket/plugin-command": "workspace:^",
    "@gasket/plugin-docs": "workspace:^",
    "@gasket/plugin-docusaurus": "workspace:^",
    "@gasket/plugin-dynamic-plugins": "workspace:^",
    "@gasket/plugin-git": "workspace:^",
    "@gasket/plugin-logger": "workspace:^",
    "@gasket/plugin-metadata": "workspace:^",
    "@gasket/request": "workspace:^",
    "@gasket/utils": "workspace:^",
    "chalk": "catalog:",
    "commander": "catalog:",
    "deepmerge": "catalog:",
    "diagnostics": "catalog:",
    "glob": "catalog:",
    "handlebars": "catalog:",
    "inquirer": "catalog:",
    "json5": "catalog:",
    "mkdirp": "catalog:",
    "ora": "catalog:",
    "semver": "catalog:"
  },
  "devDependencies": {
    "@gasket/request": "workspace:^",
    "@jest/globals": "catalog:",
    "@types/inquirer": "catalog:",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "cross-env": "catalog:",
    "dotenv": "catalog:",
    "eslint": "catalog:",
    "eslint-config-godaddy": "catalog:",
    "eslint-config-godaddy-typescript": "catalog:",
    "eslint-plugin-jest": "catalog:",
    "eslint-plugin-unicorn": "catalog:",
    "jest": "catalog:",
    "typescript": "catalog:"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "lib/*.ts"
        ],
        "extends": [
          "godaddy-typescript"
        ],
        "rules": {
          "jsdoc/*": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "test/fixtures"
  ],
  "jest": {
    "transform": {}
  }
}
