{
  "name": "create-gasket-app",
  "version": "7.2.2",
  "description": "starter pack for creating a gasket app",
  "type": "module",
  "bin": {
    "create-gasket-app": "./lib/index.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict --experimental-vm-modules' jest",
    "test:watch": "pnpm run test --watch",
    "test:coverage": "jest --coverage",
    "posttest": "pnpm run lint",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/create-gasket-app",
  "dependencies": {
    "@gasket/core": "workspace:^",
    "@gasket/plugin-command": "workspace:^",
    "@gasket/plugin-docs": "workspace:^",
    "@gasket/plugin-docusaurus": "workspace:^",
    "@gasket/plugin-dynamic-plugins": "workspace:^",
    "@gasket/plugin-git": "workspace:^",
    "@gasket/plugin-logger": "workspace:^",
    "@gasket/plugin-metadata": "workspace:^",
    "@gasket/request": "workspace:^",
    "@gasket/utils": "workspace:^",
    "commander": "^12.1.0",
    "deepmerge": "^4.3.1",
    "diagnostics": "^2.0.2",
    "glob": "^8.1.0",
    "handlebars": "^4.7.8",
    "inquirer": "6.5.2",
    "json5": "^2.2.3",
    "mkdirp": "^1.0.4",
    "ora": "3.4.0",
    "semver": "^7.7.1"
  },
  "devDependencies": {
    "@gasket/request": "7.1.0-next.6",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.19",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "catalog:",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-config-godaddy-typescript": "^4.0.3",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "typescript": "catalog:"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "lib/*.ts"
        ],
        "extends": [
          "godaddy-typescript"
        ],
        "rules": {
          "jsdoc/*": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "test/fixtures"
  ],
  "jest": {
    "transform": {}
  }
}
