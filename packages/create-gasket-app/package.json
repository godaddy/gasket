{
  "name": "create-gasket-app",
  "version": "7.0.0-next.57",
  "description": "starter pack for creating a gasket app",
  "type": "module",
  "bin": {
    "create-gasket-app": "./lib/index.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict --experimental-vm-modules' jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/create-gasket-app",
  "dependencies": {
    "@gasket/core": "7.0.0-next.57",
    "@gasket/plugin-command": "7.0.0-next.57",
    "@gasket/plugin-docs": "7.0.0-next.57",
    "@gasket/plugin-docusaurus": "7.0.0-next.57",
    "@gasket/plugin-git": "7.0.0-next.57",
    "@gasket/plugin-logger": "7.0.0-next.57",
    "@gasket/plugin-metadata": "7.0.0-next.57",
    "@gasket/utils": "7.0.0-next.57",
    "commander": "^12.0.0",
    "deepmerge": "^4.3.1",
    "glob": "^8.1.0",
    "handlebars": "^4.7.8",
    "inquirer": "6.5.2",
    "json5": "^2.2.3",
    "mkdirp": "^1.0.4",
    "ora": "3.4.0",
    "pump": "^3.0.0",
    "semver": "^7.5.4",
    "tar-fs": "^2.0.0",
    "untildify": "^4.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "diagnostics": "^2.0.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  },
  "eslintIgnore": [
    "test/fixtures"
  ],
  "jest": {
    "transform": {}
  }
}
