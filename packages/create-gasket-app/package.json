{
  "name": "create-gasket-app",
  "version": "6.45.1",
  "description": "starter pack for creating a gasket app",
  "main": "lib",
  "bin": {
    "create-gasket-app": "./lib/index.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/create-gasket-app",
  "dependencies": {
    "@gasket/cli": "^6.45.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.0.2",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^44.0.0",
    "jest": "^29.7.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  },
  "eslintIgnore": [
    "test/fixtures"
  ]
}
