    "lodash.defaultsdeep": "^4.6.0"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-godaddy": "^4.0.0",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^6.0.0",
    "jest": "^24.8.0"
  },
  "eslintConfig": {
    "extends": [
