{
  "name": "@gasket/utils",
  "version": "6.0.0-canary.0",
  "description": "Reusable utilities for Gasket internals",
  "main": "lib",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha test/*.test.js --require test/setup.js",
    "test:watch": "npm run test:runner -- --watch",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "docs": "jsdoc2md --plugin @godaddy/dmd --files lib/*.js > docs/api.md"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "utils"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-utils#readme",
  "dependencies": {
    "concat-stream": "^2.0.0",
    "cross-spawn": "^7.0.1",
    "lodash.defaultsdeep": "^4.6.1"
  },
  "devDependencies": {
    "@godaddy/dmd": "^1.0.0",
    "assume": "^2.3.0",
    "assume-sinon": "^1.1.0",
    "eslint": "^7.17.0",
    "eslint-config-godaddy": "^4.0.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-mocha": "^8.0.0",
    "jsdoc-to-markdown": "^5.0.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^9.2.3"
  },
  "eslintConfig": {
    "extends": [
      "godaddy"
    ]
  },
  "eslintIgnore": [
    "coverage",
    "test/fixtures"
  ]
}
