{
  "name": "@gasket/utils",
  "version": "6.34.4",
  "description": "Reusable utilities for Gasket internals",
  "main": "lib",
  "types": "lib/index.d.ts",
  "files": [
    "docs",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha test/*.test.js --require test/setup.js",
    "test:watch": "npm run test:runner -- --watch",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "docs": "jsdoc2md --plugin @godaddy/dmd --files lib/*.js > docs/api.md"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "utils"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-utils",
  "dependencies": {
    "concat-stream": "^2.0.0",
    "cross-spawn": "^7.0.1",
    "lodash.defaultsdeep": "^4.6.1"
  },
  "devDependencies": {
    "@gasket/engine": "^6.34.4",
    "@godaddy/dmd": "^1.0.0",
    "abort-controller": "^3.0.0",
    "assume": "^2.3.0",
    "assume-sinon": "^1.1.0",
    "eslint": "^8.7.0",
    "eslint-config-godaddy": "^6.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-unicorn": "^43.0.0",
    "jsdoc-to-markdown": "^7.1.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^14.0.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  },
  "eslintIgnore": [
    "test/fixtures"
  ]
}
