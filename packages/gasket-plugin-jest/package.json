{
  "name": "@gasket/plugin-jest",
  "version": "7.3.0",
  "description": "Integrated jest into your application.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "generator",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "jest",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-jest",
  "devDependencies": {
    "@gasket/core": "workspace:^",
    "@gasket/plugin-express": "workspace:^",
    "@gasket/plugin-metadata": "workspace:^",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "babel-eslint": "catalog:",
    "create-gasket-app": "workspace:^",
    "cross-env": "catalog:",
    "eslint": "catalog:",
    "eslint-config-godaddy": "catalog:",
    "eslint-config-godaddy-typescript": "catalog:",
    "eslint-plugin-jest": "catalog:",
    "eslint-plugin-jsx-a11y": "catalog:",
    "eslint-plugin-react": "catalog:",
    "eslint-plugin-unicorn": "catalog:",
    "jest": "catalog:",
    "jest-environment-jsdom": "catalog:",
    "next": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "ts-jest": "catalog:",
    "ts-node": "catalog:",
    "typescript": "catalog:"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "lib/*.ts"
        ],
        "extends": [
          "godaddy-typescript"
        ],
        "rules": {
          "jsdoc/*": "off"
        }
      }
    ]
  }
}
