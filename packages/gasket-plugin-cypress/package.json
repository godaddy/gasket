{
  "name": "@gasket/plugin-cypress",
  "version": "6.45.2",
  "description": "Integrates Cypress based testing into your Gasket application",
  "main": "lib",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cypress",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-cypress",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@testing-library/react": "^13.4.0",
    "cypress": "^13.6.6",
    "eslint": "^8.57.0",
    "eslint-config-godaddy": "^7.0.2",
    "eslint-plugin-unicorn": "^44.0.0",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "start-server-and-test": "^1.14.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error",
      "max-nested-callbacks": [
        2,
        6
      ]
    }
  }
}
