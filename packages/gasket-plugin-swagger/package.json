{
  "name": "@gasket/plugin-swagger",
  "version": "0.1.0",
  "description": "Generate and serve swagger docs",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha --require setup-env --recursive \"test/**/*.*(test|spec).js\"",
    "test:watch": "npm run test:runner -- --watch",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "report": "nyc report --reporter=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "swagger",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Kawika Bader <kbader@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-plugin-swagger",
  "dependencies": {
    "express": "^4.16.3",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/register": "^7.0.0",
    "assume": "^2.2.0",
    "assume-sinon": "^1.0.1",
    "eslint": "^6.1.0",
    "eslint-config-godaddy": "^4.0.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^5.3.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "setup-env": "^1.2.2",
    "sinon": "^7.4.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  }
}
