{
  "name": "@gasket/core",
  "version": "7.0.0-next.23",
  "description": "Entry point to setting up Gasket instances",
  "module": "lib/index.js",
  "main": "cjs/index.js",
  "exports": {
    "import": "./lib/index.js",
    "require": "./cjs/index.js"
  },
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "cjs"
  ],
  "scripts": {
    "build": "swc lib -d cjs --delete-dir-on-start --strip-leading-paths",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict --experimental-vm-modules' jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "posttest": "npm run lint && npm run typecheck",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "preset",
    "plugin",
    "plugin-engine"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Jacob Page <jpage@godaddy.com>",
    "Charlie Robbins <charlie.robbins@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-engine",
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.1.0",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^44.0.0",
    "jest": "^29.7.0",
    "typescript": "^5.4.4"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "cjs/"
    ],
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error",
      "no-undefined": 0,
      "no-sync": 0
    }
  },
  "type": "module"
}
