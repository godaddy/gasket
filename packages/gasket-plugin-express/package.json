{
  "name": "@gasket/plugin-express",
  "version": "7.2.3",
  "description": "Adds express support to your application",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "generator",
    "docs",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "express",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-express",
  "dependencies": {
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "diagnostics": "^2.0.2",
    "glob": "^8.1.0",
    "http2-express": "^1.0.1"
  },
  "devDependencies": {
    "@gasket/core": "workspace:^",
    "@gasket/plugin-https": "workspace:^",
    "@gasket/plugin-logger": "workspace:^",
    "@gasket/plugin-metadata": "workspace:^",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.19",
    "create-gasket-app": "workspace:^",
    "cross-env": "^7.0.3",
    "eslint": "catalog:",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-config-godaddy-typescript": "^4.0.3",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "express": "^4.21.2",
    "jest": "^29.7.0",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "express": "^4.21.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "lib/*.ts"
        ],
        "extends": [
          "godaddy-typescript"
        ],
        "rules": {
          "jsdoc/*": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "generator"
  ],
  "jest": {
    "rootDir": "test"
  }
}
