{
  "name": "@gasket/plugin-express",
  "version": "7.0.0-next.53",
  "description": "Adds express support to your application",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "generator",
    "docs",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint && npm run typecheck",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "express",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-express",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "diagnostics": "^2.0.2",
    "glob": "^8.1.0",
    "http2-express-bridge": "^1.0.7"
  },
  "devDependencies": {
    "@gasket/core": "7.0.0-next.53",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "express": "^4.16.3"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error",
      "valid-jsdoc": "off",
      "spaced-comment": [
        "error",
        "always",
        {
          "markers": [
            "/"
          ]
        }
      ]
    }
  },
  "eslintIgnore": [
    "generator"
  ]
}
