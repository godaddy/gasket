{
  "name": "@gasket/plugin-happyfeet",
  "version": "7.3.5",
  "description": "A gasket plugin to enable happyfeet healthchecks",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": "godaddy/gasket.git",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": "https://github.com/godaddy/gasket/issues",
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-happyfeet",
  "dependencies": {
    "happy-feet": "^2.2.0"
  },
  "devDependencies": {
    "@gasket/core": "workspace:^",
    "@gasket/plugin-https": "workspace:^",
    "@gasket/plugin-metadata": "workspace:^",
    "@godaddy/terminus": "^4.12.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.19",
    "create-gasket-app": "workspace:^",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-config-godaddy-typescript": "^4.0.3",
    "eslint-plugin-jest": "^29.0.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "typescript": "^5.8.2"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "lib/*.ts"
        ],
        "extends": [
          "godaddy-typescript"
        ],
        "rules": {
          "jsdoc/*": "off"
        }
      }
    ]
  }
}
