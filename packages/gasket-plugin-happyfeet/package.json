{
  "name": "@gasket/plugin-happyfeet",
  "version": "8.0.0",
  "description": "A gasket plugin to enable happyfeet healthchecks",
  "type": "module",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "EXAMPLES.md"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint .",
