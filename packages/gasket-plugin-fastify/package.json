{
  "name": "@gasket/plugin-fastify",
  "version": "7.5.4",
  "description": "Adds fastify support to your application",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "cjs",
    "generator",
    "lib",
    "EXAMPLES.md"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "gasket-cjs ./lib",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "prepack": "pnpm run build",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": "godaddy/gasket.git",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "fastify",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": "https://github.com/godaddy/gasket/issues",
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-fastify",
  "dependencies": {
    "fastify": "^4.29.1"
  },
  "devDependencies": {
    "@gasket/cjs": "workspace:^",
    "@gasket/core": "workspace:^",
    "@gasket/plugin-https": "workspace:^",
    "@gasket/plugin-logger": "workspace:^",
    "@gasket/plugin-metadata": "workspace:^",
    "@types/connect": "^3.4.38",
    "create-gasket-app": "workspace:^",
    "fastify": "^4.29.1",
    "vitest": "^3.2.0"
  },
  "peerDependencies": {
    "fastify": "^4.29.1"
  }
}
