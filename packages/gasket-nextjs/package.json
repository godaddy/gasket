{
  "name": "@gasket/nextjs",
  "version": "6.0.0",
  "description": "Gasket integrations for Next.js apps",
  "main": "./lib",
  "browser": "./lib",
  "module": "./src",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha --require setup-env \"test/*.test.js\"",
    "test:watch": "npm run test:runner -- --watch",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "build": "babel src -d lib",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "nextjs"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-nextjs#readme",
  "dependencies": {
    "htmlescape": "^1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/register": "^7.12.10",
    "assume": "^2.3.0",
    "assume-sinon": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.17.0",
    "eslint-config-godaddy-react": "^6.0.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-react": "^7.22.0",
    "jsdom": "^16.4.0",
    "mocha": "^8.2.1",
    "next": "^10.0.5",
    "nyc": "^15.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "setup-env": "^1.2.2",
    "sinon": "^9.2.3"
  },
  "peerDependencies": {
    "next": "^10.0.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy-react"
    ]
  },
  "eslintIgnore": [
    "lib",
    "coverage"
  ]
}
