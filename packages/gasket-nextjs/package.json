{
  "name": "@gasket/nextjs",
  "version": "7.0.0-canary.0",
  "description": "Gasket integrations for Next.js apps",
  "main": "lib",
  "browser": "lib",
  "module": "src",
  "types": "src/index.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint",
    "build": "babel src -d lib --delete-dir-on-start",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "nextjs"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-nextjs",
  "dependencies": {
    "@gasket/data": "^7.0.0-canary.0",
    "htmlescape": "^1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@testing-library/react": "^13.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-godaddy-react": "^9.0.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-unicorn": "^44.0.0",
    "jest": "^29.7.0",
    "jsdom": "^20.0.0",
    "next": "13.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "next": ">=10.2.0 <= 13.1.1",
    "prop-types": "^15",
    "react": "^16 || ^17 || ^18"
  },
  "eslintConfig": {
    "extends": [
      "godaddy-react",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  },
  "eslintIgnore": [
    "lib"
  ],
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom"
  }
}
