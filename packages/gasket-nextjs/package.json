{
  "name": "@gasket/nextjs",
  "version": "6.42.0",
  "description": "Gasket integrations for Next.js apps",
  "main": "lib",
  "browser": "lib",
  "module": "src",
  "types": "src/index.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha --require setup-env \"test/*.test.js\"",
    "test:watch": "npm run test:runner -- --watch",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "build": "babel src -d lib --delete-dir-on-start",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "nextjs"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-nextjs",
  "dependencies": {
    "@gasket/data": "^6.41.1",
    "htmlescape": "^1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.16.10",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/register": "^7.12.10",
    "@testing-library/react": "^13.4.0",
    "assume": "^2.3.0",
    "assume-sinon": "^1.1.0",
    "decache": "^4.6.1",
    "eslint": "^8.7.0",
    "eslint-config-godaddy-react": "^9.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-unicorn": "^44.0.0",
    "jsdom": "^20.0.0",
    "mocha": "^10.0.0",
    "mock-require": "^3.0.3",
    "next": "13.1.1",
    "nyc": "^15.1.0",
    "prop-types": "^15.7.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "setup-env": "^2.0.0",
    "sinon": "^14.0.0"
  },
  "peerDependencies": {
    "next": ">=10.2.0 <= 13.1.1",
    "prop-types": "^15",
    "react": "^16 || ^17 || ^18"
  },
  "eslintConfig": {
    "extends": [
      "godaddy-react"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  },
  "eslintIgnore": [
    "lib"
  ],
  "babel": {
    "presets": [
      "next/babel"
    ]
  }
}
