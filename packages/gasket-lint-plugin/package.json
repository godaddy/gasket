{
  "name": "@gasket/lint-plugin",
  "version": "1.7.1-canary.0",
  "description": "Adds GoDaddy standard linting to your application",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js test.js",
    "lint:fix": "npm run lint -- --fix",
    "posttest": "npm run lint",
    "test": "mocha test.js",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test",
    "test:watch": "mocha  --watch test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "keywords": [
    "gasket",
    "plugin",
    "lint",
    "eslint"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-lint-plugin#readme",
  "devDependencies": {
    "assume": "^2.1.0",
    "assume-sinon": "^1.0.0",
    "eslint": "^5.13.0",
    "eslint-config-godaddy": "^3.0.0",
    "eslint-plugin-json": "^1.3.2",
    "eslint-plugin-mocha": "^5.2.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "sinon": "^7.2.3"
  }
}
