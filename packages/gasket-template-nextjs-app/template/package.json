{
  "name": "{{{appName}}}",
  "version": "0.0.0",
  "description": "Gasket App",
  "scripts": {
    "docs": "tsx gasket.ts docs",
    "build": "npm run build:tsc && next build",
    "start": "npm run start:https & next start",
    "local": "concurrently \"npm run build:tsc:watch\" \"npm run local:https\" \"next dev\"",
    "preview": "npm run build && npm run start",
    "prebuild": "tsx gasket.ts build",
    "start:https": "node dist/server.js",
    "local:https": "tsx watch server.ts",
    "build:tsc:watch": "tsc -p ./tsconfig.server.json --watch",
    "build:tsc": "tsc -p ./tsconfig.server.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --ext .js,.jsx,.cjs,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "@gasket/assets": "next",
    "@gasket/core": "next",
    "@gasket/intl": "next",
    "@gasket/nextjs": "next",
    "@gasket/plugin-command": "next",
    "@gasket/plugin-dynamic-plugins": "next",
    "@gasket/plugin-https-proxy": "next",
    "@gasket/plugin-intl": "next",
    "@gasket/plugin-logger": "next",
    "@gasket/plugin-nextjs": "next",
    "@gasket/plugin-webpack": "next",
    "@gasket/plugin-winston": "next",
    "@gasket/request": "next",
    "@gasket/utils": "next",
    "@types/react": "^19.0.12",
    "next": "^15.5.9",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-intl": "^7.1.6",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": ">=7",
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@gasket/plugin-docs": "next",
    "@gasket/plugin-docusaurus": "next",
    "@gasket/plugin-metadata": "next",
    "@godaddy/eslint-plugin-react-intl": "^1.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.0",
    "ajv": "^8.17.1",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "eslint-config-godaddy-react": "^9.1.0",
    "eslint-config-next": "^13.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^20.0.3",
    "search-insights": "^2.17.3",
    "typescript": "^5.8.2",
    "vitest": "^3.2.0",
    "webpack": "^5.98.0"
  },
  "type": "module",
  "eslintIgnore": [
    "dist",
    "coverage/",
    "build/"
  ],
  "eslintConfig": {
    "extends": [
      "godaddy-react",
      "plugin:@godaddy/react-intl/recommended",
      "next"
    ],
    "settings": {
      "localeFiles": [
        "locales/en-US.json"
      ]
    },
    "parser": "@typescript-eslint/parser"
  }
}
