{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": [
    "@tsconfig/node20/tsconfig.json"
  ],
  "compilerOptions": {
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ESNext",
    "isolatedModules": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "lib": [
      "esnext"
    ],
    "types": [
      "node"
    ],
    "baseUrl": "./"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./routes",
    "./plugins",
    "server.ts",
    "gasket.ts"
  ]
}
