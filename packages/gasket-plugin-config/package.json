{
  "name": "@gasket/plugin-config",
  "version": "6.2.0",
  "description": "Supports application-specific (non-Gasket) configuration",
  "main": "lib/plugin.js",
  "types": "lib/plugin.d.ts",
  "scripts": {
    "lint": "eslint ./lib ./test",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "plugin",
    "config"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Jacob Page <jpage@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-plugin-config",
  "dependencies": {
    "@gasket/utils": "^6.2.0",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.merge": "^4.6.0"
  },
  "devDependencies": {
    "@gasket/engine": "^6.7.3",
    "eslint": "^7.17.0",
    "eslint-config-godaddy": "^4.0.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "jest": "^27.4.3",
    "sinon": "^12.0.1"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  }
}
