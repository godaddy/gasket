{
  "name": "@gasket/git-plugin",
  "version": "1.0.0",
  "description": "Adds git support to your application",
  "main": "lib",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "test": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "test:runner": "mocha --require ./test/setup.js \"test/**/*.*(test|spec).js\"",
    "report": "nyc report --reporter=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "keywords": [
    "git",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-git-plugin",
  "dependencies": {
    "@gasket/utils": "^1.1.0"
  },
  "devDependencies": {
    "assume": "^2.1.0",
    "assume-sinon": "^1.0.0",
    "eslint": "^6.1.0",
    "eslint-config-godaddy": "^4.0.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^6.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "proxyquire": "^2.1.0",
    "sinon": "^7.2.3"
  },
  "eslintConfig": {
    "extends": [
      "godaddy"
    ]
  },
  "eslintIgnore": [
    "coverage"
  ]
}
