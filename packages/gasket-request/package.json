{
  "name": "@gasket/request",
  "version": "7.2.0",
  "description": "Utilities for working with request objects in Gasket",
  "type": "module",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "cjs",
    "docs"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./cjs/index.cjs",
      "types": "./lib/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict --experimental-vm-modules' jest",
    "test:watch": "pnpm run test --watch",
    "test:coverage": "pnpm run test --coverage",
    "posttest": "pnpm run lint",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch",
    "build": "swc lib -d cjs --delete-dir-on-start --strip-leading-paths --out-file-extension cjs",
    "build:watch": "pnpm run build --watch",
    "postbuild": "pnpm run postbuild:package && pnpm run postbuild:replace",
    "prepublishOnly": "pnpm run build",
    "postbuild:package": "node -e \"require('fs').writeFileSync('cjs/package.json', '{}')\"",
    "postbuild:replace": "replace '.js' '.cjs' cjs/*"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "preset",
    "plugin",
    "plugin-engine"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-request",
  "dependencies": {
    "debug": "^4.4.0"
  },
  "devDependencies": {
    "@gasket/core": "workspace:^",
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.3.14",
    "@swc/core": "^1.10.18",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.19",
    "cross-env": "^7.0.3",
    "eslint": "catalog:",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-config-godaddy-typescript": "^4.0.3",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "replace": "^1.2.2",
    "typescript": "catalog:"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "cjs/"
    ],
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error",
      "no-undefined": 0,
      "no-sync": 0
    },
    "overrides": [
      {
        "files": [
          "test/**/*.js"
        ],
        "rules": {
          "jsdoc/require-jsdoc": "off"
        }
      },
      {
        "files": [
          "lib/*.ts"
        ],
        "extends": [
          "godaddy-typescript"
        ],
        "rules": {
          "jsdoc/*": "off"
        }
      }
    ]
  }
}
