{
  "name": "@gasket/plugin-winston",
  "version": "6.43.0",
  "description": "Gasket logger based on Winston",
  "main": "lib",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "plugin",
    "logging",
    "winston"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-winston",
  "peerDependencies": {
    "@gasket/plugin-logger": "^6.0.0"
  },
  "dependencies": {
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@gasket/cli": "^6.43.1",
    "@gasket/engine": "^6.43.0",
    "@gasket/plugin-logger": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-config-godaddy": "^7.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "jest": "^29.7.0",
    "triple-beam": "^1.4.1"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  }
}
