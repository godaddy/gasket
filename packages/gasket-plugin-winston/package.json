{
  "name": "@gasket/plugin-winston",
  "version": "7.3.3",
  "description": "Gasket logger based on Winston",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:coverage": "pnpm run test --coverage",
    "test:watch": "pnpm run test --watch",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": "godaddy/gasket.git",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "logging",
    "plugin",
    "winston"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": "https://github.com/godaddy/gasket/issues",
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-winston",
  "dependencies": {
    "@gasket/plugin-logger": "workspace:*",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@gasket/core": "workspace:*",
    "@gasket/plugin-metadata": "workspace:*",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.19",
    "babel-eslint": "^10.1.0",
    "create-gasket-app": "workspace:*",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-config-godaddy-typescript": "^4.0.3",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "triple-beam": "^1.4.1",
    "typescript": "^5.8.2"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "lib/*.ts"
        ],
        "extends": [
          "godaddy-typescript"
        ],
        "rules": {
          "jsdoc/*": "off"
        }
      }
    ]
  }
}
