{
  "plugins": [
    "jira",
    {
      "plugin": "requiredFile",
      "config": {
        "files": [
          {
            "path": "CHANGELOG.md",
            "message": "A `CHANGELOG.md` entry is required for changes to this repo. Please add an entry unless you have a good reason not to."
          }
        ]
      }
    },
    {
      "plugin": "reviewers",
      "config": {
        "howMany": 2
      }
    }
  ]
}
