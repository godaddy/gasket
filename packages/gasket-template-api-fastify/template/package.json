{
  "name": "{{{appName}}}",
  "version": "0.0.0",
  "description": "Gasket App",
  "scripts": {
    "docs": "tsx gasket.ts docs",
    "prebuild": "tsx gasket.ts build",
    "build": "tsc",
    "preview": "npm run build && npm run start",
    "start": "node dist/server.js",
    "local": "tsx watch server.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --ext .js,.jsx,.cjs,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "@gasket/core": "next",
    "@gasket/plugin-command": "next",
    "@gasket/plugin-dynamic-plugins": "next",
    "@gasket/plugin-fastify": "next",
    "@gasket/plugin-https": "next",
    "@gasket/plugin-logger": "next",
    "@gasket/plugin-swagger": "next",
    "@gasket/plugin-winston": "next",
    "@gasket/request": "next",
    "@gasket/utils": "next",
    "fastify": "^5.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@gasket/plugin-docs": "next",
    "@gasket/plugin-docusaurus": "next",
    "@gasket/plugin-metadata": "next",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.0",
    "ajv": "^8.17.1",
    "eslint": "^8.57.1",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "search-insights": "^2.17.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.2.0"
  },
  "type": "module",
  "eslintIgnore": [
    "dist",
    "coverage/",
    "build/"
  ],
  "eslintConfig": {
    "extends": [
      "godaddy"
    ],
    "parser": "@typescript-eslint/parser"
  }
}
