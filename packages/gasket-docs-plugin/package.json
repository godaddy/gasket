{
  "name": "@gasket/docs-plugin",
  "version": "1.0.0-canary.0",
  "description": "Centralize doc files from plugins and modules",
  "main": "lib",
  "scripts": {
    "lint": "eslint lib test",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha --recursive --require test/setup.js test",
    "test:watch": "npm run test:runner -- --watch",
    "test:coverage": "nyc --reporter=text --reporter=lcov --all npm run test:runner",
    "posttest": "npm run lint",
    "prepack": "npm run docs",
    "docs": "jsdoc2md lib/*.js > docs/api.md"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "keywords": [
    "docs",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-docs-plugin",
  "dependencies": {
    "glob": "^7.1.4",
    "lodash.defaultsdeep": "^4.6.0",
    "markdown-table": "^1.1.3",
    "mkdirp": "^0.5.1",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "assume": "^2.2.0",
    "eslint": "^6.1.0",
    "eslint-config-godaddy": "^4.0.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^6.0.0",
    "jsdoc-to-markdown": "^5.0.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "sinon": "^7.4.1"
  }
}
