{
  "name": "@gasket/plugin-analyze",
  "version": "8.0.0",
  "description": "Gasket Analyzer Plugin",
  "type": "module",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "EXAMPLES.md"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
