{
  "name": "@gasket/plugin-command",
  "version": "6.45.2",
  "description": "Plugin to enable other plugins to inject new gasket commands",
  "main": "lib/plugin.js",
  "types": "lib/plugin.d.ts",
  "files": [
    "lib",
    "docs"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint",
    "prepack": "npm run docs",
    "docs": "jsdoc2md --plugin @godaddy/dmd --files lib/*.js > docs/api.md"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "plugin",
    "oclif",
    "command"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Jacob Page <jpage@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-command",
  "dependencies": {
    "@gasket/utils": "^6.45.2",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@gasket/engine": "^6.45.2",
    "@godaddy/dmd": "^1.0.4",
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.2",
    "@oclif/errors": "^1.3.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.0.2",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^44.0.0",
    "jest": "^29.7.0",
    "jsdoc-to-markdown": "^7.1.0"
  },
  "peerDependencies": {
    "@oclif/config": "^1.13.0",
    "@oclif/errors": "^1.1.2"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  }
}
