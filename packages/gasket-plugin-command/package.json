{
  "name": "@gasket/plugin-command",
  "version": "6.2.0",
  "description": "Plugin to enable other plugins to inject new gasket commands",
  "main": "lib/plugin.js",
  "types": "lib/plugin.d.ts",
  "scripts": {
    "lint": "eslint ./lib ./test",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha test/*.test.js --require test/setup.js",
    "test:watch": "npm run test:runner -- --watch",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "prepack": "npm run docs",
    "docs": "jsdoc2md --plugin @godaddy/dmd --files lib/*.js > docs/api.md"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "plugin",
    "oclif",
    "command"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Jacob Page <jpage@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-plugin-command",
  "dependencies": {
    "@gasket/utils": "^6.2.0"
  },
  "devDependencies": {
    "@gasket/engine": "^6.7.3",
    "@godaddy/dmd": "^1.0.0",
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.15.0",
    "@oclif/errors": "^1.1.2",
    "assume": "^2.3.0",
    "assume-sinon": "^1.1.0",
    "eslint": "^7.17.0",
    "eslint-config-godaddy": "^4.0.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "jsdoc-to-markdown": "^5.0.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^9.2.3"
  },
  "peerDependencies": {
    "@oclif/config": "^1.13.0",
    "@oclif/errors": "^1.1.2"
  },
  "eslintConfig": {
    "extends": [
      "godaddy"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  }
}
