{
  "name": "@gasket/plugin-elastic-apm",
  "version": "7.0.0-next.51",
  "description": "Adds Elastic APM instrumentation to your application",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "jest test/*.test.js",
    "test:watch": "npm run test:runner -- --watch",
    "test:coverage": "npm run test:runner -- --coverage --coverageReporters text",
    "posttest": "npm run lint && npm run typecheck",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "elastic",
    "apm",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Keith Bartholomew <kbartholomew@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-elastic-apm",
  "devDependencies": {
    "@gasket/core": "7.0.0-next.51",
    "dotenv": "^16.4.5",
    "elastic-apm-node": "^4.4.1",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "nyc": "^15.1.0",
    "typescript": "^5.4.5"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
