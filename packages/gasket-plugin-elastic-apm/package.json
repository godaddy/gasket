{
  "name": "@gasket/plugin-elastic-apm",
  "version": "7.4.0",
  "description": "Adds Elastic APM instrumentation to your application",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:coverage": "pnpm run test --coverage",
    "test:watch": "pnpm run test --watch",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": "godaddy/gasket.git",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "apm",
    "elastic",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": "https://github.com/godaddy/gasket/issues",
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-elastic-apm",
  "dependencies": {
    "@gasket/request": "workspace:*"
  },
  "devDependencies": {
    "@gasket/core": "workspace:*",
    "@gasket/plugin-metadata": "workspace:*",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.19",
    "create-gasket-app": "workspace:*",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "elastic-apm-node": "^4.11.0",
    "eslint": "^8.57.1",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-config-godaddy-typescript": "^4.0.3",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "nyc": "^15.1.0",
    "typescript": "^5.8.2"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "lib/*.ts"
        ],
        "extends": [
          "godaddy-typescript"
        ],
        "rules": {
          "jsdoc/*": "off"
        }
      }
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
