{
  "name": "@gasket/intl",
  "version": "6.0.0-canary.3",
  "description": "React component library to enable localization for gasket apps.",
  "main": "./lib",
  "browser": "./lib",
  "module": "./src",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha --require setup-env --require test/setup.js \"test/*.test.js\"",
    "test:watch": "npm run test:runner -- --watch",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "build": "babel src -d lib",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "intl"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Salil Agrawal <sagrawal@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-intl#readme",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@gasket/fetch": "^6.0.0-canary.2",
    "@gasket/helper-intl": "^6.0.0-canary.3",
    "hoist-non-react-statics": "^2.5.0",
    "prop-types": "^15.6.2",
    "reduxful": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.3.1",
    "assume": "^2.2.0",
    "assume-sinon": "^1.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-root-import": "^5.1.0",
    "enzyme": "^3.10.0",
    "eslint": "^6.1.0",
    "eslint-config-godaddy-react": "^6.0.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^6.0.0",
    "eslint-plugin-react": "^7.14.0",
    "intl": "^1.2.5",
    "jsdom": "^16.4.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intl": "^5.8.5",
    "react-redux": "^7.1.0",
    "setup-env": "^1.2.2",
    "sinon": "^7.4.1"
  },
  "peerDependencies": {
    "@gasket/plugin-intl": "^6.0.0",
    "react-intl": ">=2.0.0 <6.0.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy-react"
    ]
  },
  "eslintIgnore": [
    "lib",
    "coverage"
  ]
