{
  "name": "@gasket/plugin-redux",
  "version": "7.3.0",
  "description": "DEPRECATED Gasket Redux Setup",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "generator",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict --experimental-vm-modules' jest",
    "test:watch": "pnpm run test --watchAll",
    "test:coverage": "pnpm run test --coverage",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "plugin",
    "redux"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-redux",
  "devDependencies": {
    "@gasket/core": "workspace:^",
    "@gasket/plugin-express": "workspace:^",
    "@gasket/plugin-logger": "workspace:^",
    "@gasket/plugin-metadata": "workspace:^",
    "@gasket/plugin-middleware": "workspace:^",
    "@gasket/plugin-webpack": "workspace:^",
    "@gasket/redux": "workspace:^",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "acorn": "catalog:",
    "create-gasket-app": "workspace:^",
    "cross-env": "catalog:",
    "eslint": "catalog:",
    "eslint-config-godaddy": "catalog:",
    "eslint-config-godaddy-typescript": "catalog:",
    "eslint-plugin-jest": "catalog:",
    "eslint-plugin-unicorn": "catalog:",
    "jest": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-redux": "catalog:",
    "redux": "catalog:",
    "typescript": "catalog:",
    "webpack": "catalog:"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "globals": {
      "expect": "readonly"
    },
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "lib/*.ts"
        ],
        "extends": [
          "godaddy-typescript"
        ],
        "rules": {
          "jsdoc/*": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "generator/redux/store.js"
  ],
  "deprecated": "Package deprecated and will be removed in future major release."
}
