  "keywords": [
    "gasket",
    "plugin",
    "redux"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-plugin-redux",
  "devDependencies": {
    "@gasket/engine": "^6.7.3",
    "@gasket/plugin-log": "^6.1.0",
    "@gasket/redux": "^6.1.0",
    "acorn": "^6.1.1",
    "eslint": "^7.17.0",
    "eslint-config-godaddy": "^4.0.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "jest": "^27.4.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.1.0",
    "redux": "^4.0.5",
    "webpack": "^5.21.2"
  },
  "peerDependencies": {
    "@gasket/plugin-log": "^6.0.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended"
    ],
    "globals": {
      "expect": "readonly"
    },
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  },
  "eslintIgnore": [
    "coverage",
    "generator/redux/store.js"
  ]
