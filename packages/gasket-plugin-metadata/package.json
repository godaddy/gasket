{
  "name": "@gasket/plugin-metadata",
  "version": "7.1.0-canary.3",
  "description": "Adds metadata to gasket lifecycles",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest --watch=false --globals",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "posttest": "npm run lint && npm run typecheck",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "metadata",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-metadata",
  "dependencies": {
    "@gasket/core": "7.1.0-canary.3",
    "@gasket/plugin-logger": "7.1.0-canary.3"
  },
  "devDependencies": {
    "@godaddy/dmd": "^1.0.4",
    "@types/node": "^20.12.5",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "typescript": "^5.4.5",
    "vitest": "^2.1.5"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  }
}
