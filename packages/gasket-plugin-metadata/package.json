{
  "name": "@gasket/plugin-metadata",
  "version": "6.3.0",
  "description": "Adds metadata to gasket lifecycles",
  "main": "lib",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint lib test",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha test/*.test.js --require test/setup.js",
    "test:watch": "npm run test:runner -- --watch",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "prepack": "npm run docs",
    "docs": "jsdoc2md --plugin @godaddy/dmd --files lib/*.js > docs/api.md"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "metadata",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-plugin-metadata",
  "dependencies": {
    "@gasket/resolve": "^6.3.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isfunction": "^3.0.9",
    "lodash.isobject": "^3.0.2"
  },
  "devDependencies": {
    "@gasket/engine": "^6.7.3",
    "@godaddy/dmd": "^1.0.0",
    "assume": "^2.3.0",
    "assume-sinon": "^1.1.0",
    "eslint": "^7.17.0",
    "eslint-config-godaddy": "^4.0.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "jsdoc-to-markdown": "^5.0.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "sinon": "^9.2.3"
  },
  "eslintConfig": {
    "extends": [
      "godaddy"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  }
}
