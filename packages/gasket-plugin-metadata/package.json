{
  "name": "@gasket/plugin-metadata",
  "version": "7.3.0",
  "description": "Adds metadata to gasket lifecycles",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "test": "vitest run --globals",
    "test:watch": "vitest --watch --globals",
    "test:coverage": "pnpm run test --coverage",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "metadata",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-metadata",
  "dependencies": {
    "@gasket/core": "workspace:^",
    "@gasket/plugin-logger": "workspace:^"
  },
  "devDependencies": {
    "@gasket/plugin-metadata": "workspace:^",
    "@gasket/plugin-webpack": "workspace:^",
    "@godaddy/dmd": "catalog:",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "create-gasket-app": "workspace:^",
    "eslint": "catalog:",
    "eslint-config-godaddy": "catalog:",
    "eslint-config-godaddy-typescript": "catalog:",
    "eslint-plugin-jest": "catalog:",
    "eslint-plugin-unicorn": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "lib/*.ts"
        ],
        "extends": [
          "godaddy-typescript"
        ],
        "rules": {
          "jsdoc/*": "off"
        }
      }
    ]
  }
}
