{
  "name": "@gasket/plugin-metadata",
  "version": "7.3.5",
  "description": "Adds metadata to gasket lifecycles",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "test": "vitest run --globals",
    "test:coverage": "pnpm run test --coverage",
    "test:watch": "vitest --watch --globals",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": "godaddy/gasket.git",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "metadata",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": "https://github.com/godaddy/gasket/issues",
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-metadata",
  "dependencies": {
    "@gasket/core": "workspace:*",
    "@gasket/plugin-logger": "workspace:*"
  },
  "devDependencies": {
    "@gasket/plugin-metadata": "workspace:*",
    "@gasket/plugin-webpack": "workspace:*",
    "@godaddy/dmd": "^1.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.19",
    "create-gasket-app": "workspace:*",
    "eslint": "^8.57.1",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-config-godaddy-typescript": "^4.0.3",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "typescript": "5.3.3",
    "vitest": "^3.0.7"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "lib/*.ts"
        ],
        "extends": [
          "godaddy-typescript"
        ],
        "rules": {
          "jsdoc/*": "off"
        }
      }
    ]
  }
}
