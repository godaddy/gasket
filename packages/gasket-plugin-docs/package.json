{
  "name": "@gasket/plugin-docs",
  "version": "6.38.5",
  "description": "Centralize doc files from plugins and modules",
  "main": "lib",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "docs"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint",
    "prepack": "npm run docs",
    "docs": "jsdoc2md --plugin @godaddy/dmd --files lib/*.js > docs/api.md"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "docs",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-docs",
  "dependencies": {
    "glob": "^8.0.0",
    "lodash.defaultsdeep": "^4.6.1",
    "markdown-table": "^1.1.3",
    "mkdirp": "^3.0.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@gasket/engine": "^6.38.5",
    "@gasket/plugin-metadata": "^6.38.5",
    "@godaddy/dmd": "^1.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-godaddy": "^7.0.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^44.0.0",
    "jest": "^29.3.1",
    "jsdoc-to-markdown": "^7.1.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
