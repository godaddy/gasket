{
  "name": "@gasket/plugin-start",
  "version": "6.0.0-canary.0",
  "description": "Adds commands for building and starting Gasket apps",
  "main": "lib",
  "scripts": {
    "lint": "eslint lib test",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "mocha test/*.test.js --require test/setup.js",
    "test:watch": "npm run test:runner -- --watch",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "report": "nyc report --reporter=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "deploy",
    "build",
    "start",
    "local",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-plugin-start",
  "devDependencies": {
    "assume": "^2.3.0",
    "eslint": "^7.17.0",
    "eslint-config-godaddy": "^4.0.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-mocha": "^8.0.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "sinon": "^9.2.3"
  }
}
