{
  "name": "@gasket/cli",
  "version": "2.6.1",
  "description": "CLI for rapid application development with gasket",
  "main": "src/index.js",
  "bin": {
    "gasket": "./bin/boot"
  },
  "files": [
    "bin/",
    "src/",
    "oclif.manifest.json"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run test:clean",
    "test": "npm run test:unit",
    "test:coverage": "npm run test:unit:coverage",
    "posttest": "npm run lint",
    "prepack": "npm run manifest && npm run readme",
    "postpack": "rm -f oclif.manifest.json",
    "manifest": "cross-env FAUX_ROOT='/path/to/your/app' oclif-dev manifest",
    "readme": "cross-env FAUX_ROOT='/path/to/your/app' oclif-dev readme",
    "test:clean": "rimraf ./test/fixtures/create/myapp",
    "test:integration": "mocha --require test/setup.js --recursive 'test/integration/**/*.test.js'",
    "test:integration:coverage": "nyc --reporter=text --reporter=json-summary --all run-s test:integration",
    "test:mocha": "mocha --require test/setup.js --recursive 'test/**/*.test.js'",
    "test:unit": "mocha --require test/setup.js --recursive 'test/unit/**/*.test.js'",
    "test:unit:coverage": "nyc --reporter=text --reporter=lcov --all run-s test:unit",
    "version": "npm run readme && git add README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "keywords": [
    "gasket",
    "cli",
    "oclif",
    "dx"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-cli",
  "dependencies": {
    "@gasket/command-plugin": "^1.1.0",
    "@gasket/git-plugin": "^1.1.0",
    "@gasket/lifecycle-plugin": "^1.1.0",
    "@gasket/metadata-plugin": "^1.0.0",
    "@gasket/plugin-engine": "^1.5.0",
    "@gasket/start-plugin": "^0.0.0",
    "@gasket/resolve": "^1.5.0",
    "@gasket/utils": "^1.2.0",
    "@oclif/command": "^1.5.14",
    "@oclif/config": "^1.13.0",
    "@oclif/parser": "^3.8.1",
    "@oclif/plugin-help": "^2.2.0",
    "@oclif/plugin-not-found": "^1.0.9",
    "@oclif/plugin-warn-if-update-available": "^1.5.1",
    "concat-stream": "^2.0.0",
    "deepmerge": "^4.0.0",
    "handlebars": "^4.0.11",
    "inquirer": "6.3.1",
    "json5": "^2.1.0",
    "map-stream": "^0.1.0",
    "mkdirp": "^0.5.1",
    "ora": "^3.4.0",
    "pump": "^3.0.0",
    "semver": "^6.2.0",
    "tar-fs": "^2.0.0",
    "untildify": "^4.0.0",
    "vinyl-fs": "^3.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.31",
    "@oclif/test": "^1.1.0",
    "assume": "^2.2.0",
    "assume-sinon": "^1.0.1",
    "chai": "^4.2.0",
    "cross-env": "^5.1.6",
    "diagnostics": "^2.0.2",
    "eslint": "^6.1.0",
    "eslint-config-godaddy": "^4.0.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^6.0.0",
    "fancy-test": "^1.2.0",
    "fetch-mock": "^7.3.3",
    "mocha": "^6.2.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "rimraf": "^2.6.3",
    "sinon": "^7.4.1",
    "std-mocks": "^1.0.1"
  },
  "eslintIgnore": [
    "coverage",
    "**/node_modules"
  ],
  "engines": {
    "node": ">= 8.0.0"
  },
  "oclif": {
    "commands": "./src/commands",
    "bin": "gasket",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ],
    "hooks": {
      "init": "./src/hooks/init"
    },
    "warn-if-update-available": {
      "timeoutInDays": 7
    }
  }
}
