{
  "name": "@gasket/fetch",
  "version": "6.0.0-canary.2",
  "description": "Gasket Fetch API",
  "main": "./node.js",
  "browser": "./browser.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:runner",
    "test:runner": "npm run test:server && npm run test:browser",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:runner",
    "posttest": "npm run lint",
    "test:browser": "mocha --require setup-env --require whatwg-fetch test/browser.spec.js",
    "test:server": "mocha --require core-js test/server.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "fetch"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/master/packages/gasket-fetch#readme",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/register": "^7.12.10",
    "assume": "^2.3.0",
    "assume-sinon": "^1.1.0",
    "chai": "^4.2.0",
    "core-js": "^3.6.5",
    "eslint": "^7.17.0",
    "eslint-config-godaddy": "^4.0.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-mocha": "^8.0.0",
    "jsdom": "^16.4.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "regenerator-runtime": "^0.13.7",
    "setup-env": "^1.2.2",
    "sinon": "^9.2.3",
    "whatwg-fetch": "3.3.1"
  },
  "eslintConfig": {
    "extends": [
      "godaddy"
    ]
  },
  "eslintIgnore": [
    "coverage"
  ]
}
