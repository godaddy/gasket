    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "assume": "^2.2.0",
    "assume-sinon": "^1.0.1",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-godaddy": "^4.0.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^6.0.0",
    "mocha": "^6.2.0",
    "proxyquire": "^2.1.3",
    "sinon": "^7.4.1",
