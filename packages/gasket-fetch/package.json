{
  "name": "@gasket/fetch",
  "version": "7.3.0",
  "description": "Gasket Fetch API",
  "main": "lib/node.js",
  "browser": "lib/browser.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "test": "pnpm run test:runner",
    "test:runner": "pnpm run test:server && pnpm run test:browser",
    "test:browser": "jest --detectOpenHandles --env=jsdom test/browser.spec.js",
    "test:server": "jest --env=node test/server.spec.js",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "fetch"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-fetch",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/plugin-transform-runtime": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/register": "^7.25.9",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.19",
    "core-js": "^3.40.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-godaddy": "^7.1.1",
    "eslint-config-godaddy-typescript": "^4.0.3",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "jsdom": "^20.0.3",
    "regenerator-runtime": "^0.14.1",
    "typescript": "^5.7.3",
    "whatwg-fetch": "3.6.20"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended",
      "plugin:jsdoc/recommended-typescript-flavor"
    ],
    "plugins": [
      "unicorn",
      "jsdoc"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "lib/*.ts"
        ],
        "extends": [
          "godaddy-typescript"
        ],
        "rules": {
          "jsdoc/*": "off"
        }
      }
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
