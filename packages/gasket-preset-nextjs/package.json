{
  "name": "@gasket/preset-nextjs",
  "version": "7.5.6",
  "description": "Basic NextJS Framework",
  "type": "module",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "generator",
    "cjs",
    "EXAMPLES.md"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./cjs/index.cjs",
      "default": "./cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "gasket-cjs ./lib",
    "build:watch": "pnpm run build --watch",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "posttest": "pnpm run lint",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run --globals",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "repository": "godaddy/gasket.git",
  "publishConfig": {
    "access": "public"
  },
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": "https://github.com/godaddy/gasket/issues",
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-preset-nextjs",
  "dependencies": {
    "@gasket/plugin-command": "canary",
    "@gasket/plugin-cypress": "canary",
    "@gasket/plugin-docs": "canary",
    "@gasket/plugin-docusaurus": "canary",
    "@gasket/plugin-dynamic-plugins": "canary",
    "@gasket/plugin-express": "canary",
    "@gasket/plugin-git": "canary",
    "@gasket/plugin-https": "canary",
    "@gasket/plugin-https-proxy": "canary",
    "@gasket/plugin-intl": "canary",
    "@gasket/plugin-jest": "canary",
    "@gasket/plugin-lint": "canary",
    "@gasket/plugin-logger": "canary",
    "@gasket/plugin-mocha": "canary",
    "@gasket/plugin-nextjs": "canary",
    "@gasket/plugin-typescript": "canary",
    "@gasket/plugin-vitest": "canary",
    "@gasket/plugin-webpack": "canary",
    "@gasket/plugin-winston": "canary",
    "@gasket/utils": "canary"
  },
  "devDependencies": {
    "@gasket/cjs": "canary",
    "@vitest/coverage-v8": "^3.2.0",
    "vitest": "^3.2.0"
  }
}
