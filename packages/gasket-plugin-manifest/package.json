{
  "name": "@gasket/plugin-manifest",
  "version": "6.45.0",
  "description": "The web app manifest for progressive Gasket applications",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "plugin",
    "progressive web app",
    "manifest"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Sivan Mehta <smehta@godaddy.com>",
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-manifest",
  "dependencies": {
    "deepmerge": "^4.3.1",
    "escape-string-regexp": "^1.0.5",
    "mkdirp": "^1.0.4"
  },
  "devDependencies": {
    "@gasket/engine": "^6.45.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-godaddy": "^7.0.2",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-unicorn": "^44.0.0",
    "jest": "^29.7.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  }
}
