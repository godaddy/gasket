{
  "name": "@gasket/plugin-https",
  "version": "8.0.0-next.1",
  "description": "Create http/s servers with graceful termination",
  "type": "module",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "EXAMPLES.md"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "posttest": "pnpm run lint && pnpm run typecheck",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' vitest run",
    "test:coverage": "pnpm run test --coverage",
    "test:watch": "vitest watch",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "repository": "godaddy/gasket.git",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "https",
    "plugin",
    "server"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": "https://github.com/godaddy/gasket/issues",
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-https",
  "dependencies": {
    "@godaddy/terminus": "^4.12.1",
    "create-servers": "^3.3.0",
    "diagnostics": "^2.0.2",
    "errs": "^0.3.2",
    "http-proxy": "^1.18.1",
    "one-time": "^1.0.0"
  },
  "devDependencies": {
    "@gasket/core": "workspace:^",
    "@gasket/plugin-logger": "workspace:^",
    "@gasket/plugin-metadata": "workspace:^",
    "@types/http-proxy": "^1.17.16",
    "@vitest/coverage-v8": "^3.2.0",
    "create-gasket-app": "workspace:^",
    "vitest": "^3.2.0"
  }
}
