{
  "name": "@gasket/plugin-logger",
  "version": "7.4.0",
  "description": "Gasket plugin for logging",
  "type": "module",
  "main": "cjs/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.mts",
  "files": [
    "lib",
    "cjs",
    "EXAMPLES.md"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.mts",
      "import": "./lib/index.mjs",
      "require": "./cjs/index.cjs",
      "default": "./cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown && echo '{}' > cjs/package.json",
    "build:watch": "tsdown --watch",
    "pretest": "pnpm run lint && pnpm run build && pnpm run typecheck",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "repository": "godaddy/gasket.git",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "logging",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": "https://github.com/godaddy/gasket/issues",
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-logger",
  "devDependencies": {
    "@gasket/core": "workspace:^",
    "@gasket/plugin-https": "workspace:^",
    "@gasket/plugin-metadata": "workspace:^",
    "@vitest/coverage-v8": "^3.2.0",
    "create-gasket-app": "workspace:^",
    "tsdown": "^0.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.0"
  }
}
