{
  "name": "@gasket/plugin-logger",
  "version": "6.43.0",
  "description": "Gasket plugin for logging",
  "main": "lib",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gasket",
    "plugin",
    "logging"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-logger",
  "devDependencies": {
    "@gasket/engine": "^6.45.2",
    "@gasket/plugin-command": "^6.45.2",
    "@gasket/plugin-https": "^6.45.2",
    "@gasket/plugin-metadata": "^6.45.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-godaddy": "^7.0.2",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-unicorn": "^44.0.0",
    "jest": "^29.7.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  }
}
