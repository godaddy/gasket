{
  "name": "app-router",
  "version": "0.0.0",
  "description": "Gasket App",
  "scripts": {
    "docs": "node gasket.js docs",
    "build": "next build",
    "start": "next start",
    "local": "next dev",
    "preview": "npm run build && npm run start",
    "prebuild": "node gasket.js build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "eslint --ext .js,.jsx,.cjs .",
    "lint:fix": "npm run lint -- --fix",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "@gasket/assets": "^7.2.0",
    "@gasket/core": "^7.2.1",
    "@gasket/intl": "^7.2.0",
    "@gasket/nextjs": "^7.2.0",
    "@gasket/plugin-command": "^7.2.2",
    "@gasket/plugin-docs": "^7.2.2",
    "@gasket/plugin-docusaurus": "^7.2.2",
    "@gasket/plugin-intl": "^7.2.2",
    "@gasket/plugin-logger": "^7.2.2",
    "@gasket/plugin-metadata": "^7.2.2",
    "@gasket/plugin-nextjs": "^7.2.2",
    "@gasket/plugin-webpack": "^7.2.2",
    "@gasket/plugin-winston": "^7.2.2",
    "@gasket/react-intl": "^7.2.0",
    "@gasket/request": "^7.2.0",
    "@gasket/utils": "^7.2.1",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "~6.6.8",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": ">=7",
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/preset-classic": "^3.7.0",
    "@godaddy/eslint-plugin-react-intl": "^1.3.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.4.0",
    "ajv": "^8.17.1",
    "eslint": "^8.57.1",
    "eslint-config-godaddy-react": "^9.1.0",
    "eslint-config-next": "^13.4.19",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "search-insights": "^2.13.0",
    "typescript": "^5.4.5",
    "webpack": "^5.94.0"
  },
  "type": "module",
  "eslintConfig": {
    "extends": [
      "godaddy-react",
      "plugin:@godaddy/react-intl/recommended",
      "next"
    ],
    "settings": {
      "localeFiles": [
        "locales/en-US.json"
      ]
    },
    "env": {
      "jest": true
    }
  },
  "eslintIgnore": [
    "coverage/",
    "build/"
  ]
}