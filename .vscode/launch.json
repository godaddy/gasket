  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "gasket-cli create",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/packages/gasket-cli/bin/run",
      "cwd": "${workspaceFolder}/packages/gasket-cli",
      "args": [
        "create",
        "app",
        "--presets",
        "@godaddy/webapp"
      ],
      "console": "integratedTerminal"
    }
  ]
}